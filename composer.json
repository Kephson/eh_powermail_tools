{
	"name": "ehaerer/eh-powermail-tools",
	"description": "Adds some tools e.g. to export data from Powermail backend module with one click.",
	"license": "GPL-2.0+",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3 CMS",
		"export",
		"powermail",
		"mails",
		"backend"
	],
	"homepage": "https://ephra.im",
	"require": {
		"php": "^7.2 || ^8.0",
		"in2code/powermail": "^8.2",
		"typo3/cms-backend": "^10.4",
		"typo3/cms-core": "^10.4"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.28"
	},
	"autoload": {
		"psr-4": {
			"EHAERER\\EhPowermailTools\\": "Classes"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true,
			"ergebnis/composer-normalize": true
		}
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"extension-key": "eh_powermail_tools",
			"web-dir": ".Build/web"
		}
	}
}
